{
  "name": "vitepress-blogs-theme-template",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint:eslint": "eslint . --ext .ts,.js,.vue --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@chunge16/vitepress-blogs-theme": "^0.1.14",
    "@iconify-json/carbon": "^1.1.19",
    "@iconify-json/logos": "^1.1.34",
    "@iconify-json/twemoji": "^1.1.12",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "markdown-it": "^13.0.1",
    "tailwindcss": "^3.3.3",
    "vitepress": "1.0.0-rc.31",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.1"
  },
  "dependencies": {
    "@jcamp/tailwindcss-plugin-icons": "^0.6.2",
    "date-fns": "^2.30.0",
    "vitepress-plugin-comment-with-giscus": "^1.1.10"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "*.{cjs,js,ts,vue}": [
      "npm run lint:eslint"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  }
}
